{
  "manifest_version": 3,
  "name": "Avalab.aiプロンプトダウンロード",
  "description": "Avalab.aiにプロンプトのダウンロードリンクを挿入する拡張機能",
  "version": "1.0",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://avalab.ai/*"],
      "js": ["index.js"],
      "css": ["global.css"]
    }
  ]
}
